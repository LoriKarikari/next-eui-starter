{
  "name": "@elastic/next-eui-starter",
  "private": true,
  "description": "Start building Kibana protoypes quickly with the Next.js EUI Starter",
  "version": "1.0.0",
  "author": "Rory Hunter <rory@elastic.co>",
  "keywords": [
    "next",
    "kibana",
    "eui",
    "elastic",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn lint && rm public/themes/*.min.css && next build",
    "build-docs": "rm -rf docs && PATH_PREFIX=true yarn build && PATH_PREFIX=true next export -o docs && touch docs/.nojekyll",
    "dev": "next",
    "lint": "eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/next-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/next-eui-starter/issues"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.2",
    "@elastic/eui": "^18.2.1",
    "@next/bundle-analyzer": "^9.2.0",
    "@zeit/next-sass": "^1.0.1",
    "glob": "^7.1.6",
    "moment": "^2.24.0",
    "next": "^9.2.0",
    "next-images": "^1.3.0",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "null-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.5"
  }
}
